        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.kulv.kulvrss.ItemListActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.kulv.kulvrss.ArtlstListActivity"
            android:label="@string/title_artlst_list" >
        </activity>
        <activity
            android:name="com.kulv.kulvrss.ArtlstDetailActivity"
            android:label="@string/title_artlst_detail"
            android:parentActivityName=".ArtlstListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ArtlstListActivity" />
        </activity>
        
        <activity
            android:name="com.kulv.kulvrss.test1"
